{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/**/*.tsx": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "engines": {
    "node": "18.x"
  }
}
